{
  "name": "playwright-automation-testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tests": "playwright test",
    "tests:headed": "playwright test --headed",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:report": "npm run allure:generate && npm run allure:open"
  },
  "keywords": [
    "playwright",
    "typescript"
  ],
  "author": "Denys Holiak (https://github.com/HoliakDenys)",
  "license": "ISC",
  "description": "Project for automated testing with Playwright and TypeScript",
  "devDependencies": {
    "@playwright/test": "^1.51.0",
    "@types/node": "^22.13.10",
    "allure-playwright": "^3.2.0",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
